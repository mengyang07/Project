<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/fragment_main_layout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <com.lkmy.illegal.car.widget.viewpager.AutoScrollViewPager
        android:id="@+id/fragment_main_viewpager"
        android:layout_width="wrap_content"
        android:layout_height="150dp"/>

    <com.lkmy.library.indicator.CirclePageIndicator
        android:id="@+id/fragment_main_viewpager_indicator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/fragment_main_viewpager"
        android:layout_centerInParent="true"
        android:padding="10dip"/>

</RelativeLayout>
